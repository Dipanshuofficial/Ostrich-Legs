{
  "output": {
    "filePath": "PROJECT_CONTEXT.md",
    "style": "markdown",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFiles": [
      "shared/types.ts",
      "server/src/swarm/index.ts",
      "client/src/hooks/useComputeSwarm.ts",
      "client/vite.config.ts",
      "client/src/App.css"
    ]
  },
  "ignore": {
    "useGitignore": true,
    "customPatterns": [
      "**/.git/**",
      "**/node_modules/**",
      "**/dist/**",
      "**/.next/**",
      "**/*.lock",
      "**/*.png",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.svg",
      "**/*.ico",
      "**/*.map",
      "**/*.test.ts",
      "**/__tests__/**"
    ]
  },
  "include": [
    "client/src/**/*",
    "server/src/**/*",
    "shared/**/*",
    "package.json",
    "tsconfig.json"
  ]
}
